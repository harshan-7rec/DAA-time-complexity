#include<stdio.h>
int main()
{
    int c=0;
    int n;
    scanf("%d",&n);
    int rev=0 ;
    int remainder;
    
    c++;
    while(n!=0)
    {
        c++;
        remainder=n%10;
        c++;
        rev=rev*10+remainder;
        c++;
        n/=10;
        c++;
    }
    c++;
    c++;
    printf("%d",c);
}
