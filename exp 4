#include<stdio.h>
int main()
{   
    int co=0;
    int n;
    scanf("%d",&n);
    int c=0;
    co++;
    for(int i=n/2; i<n; i++)
    {
        co++;
        for(int j=1;j<n;j=2*j)
        {
            co++;
            for(int k=1;k<n;k=k*2)
            {
                co++;
                c++;
                co++;
            }
            co++;
        }
        co++;
    }
    co++;
    printf("%d",co);
}
